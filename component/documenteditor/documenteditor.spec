        "toolbarItems" : 					{ "type" : "toolbarItem[]", "droppable": true, "tags" : { "scope" :"design", "doc": "Configure toolbar items"}, "initialValue": [
				{ "type": "heading" }, 
        		{ "type": "|" }, 
        		{ "type": "fontfamily" }, 
        		{ "type": "fontsize" }, 
        		{ "type": "fontColor" }, 
        		{ "type": "|" }, 
