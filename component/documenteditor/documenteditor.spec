        	"name" : 						{ "type" : "string", "tags": { "showInOutlineView": true } },
        	"type" : 						{ "type" : "string", "values": [
        		"heading", 
        		"fontfamily", 
        		"fontsize", 
        		"fontColor", 
        		"fontBackgroundColor", 
        		"bold",
        		"italic",
        		"underline",
        		"strikethrough",
        		"highlight",
        		"alignment",
        		"numberedList",
        		"bulletedList",
        		"TodoList",
        		"indent",
        		"outdent",
        		"link",
        		"pageBreak",
        		"blockquote",
        		"imageUpload",
                "imageInsert",
        		"insertTable",
        		"mediaEmbed",
        		"specialCharacters",
        		"undo",
        		"redo",
        		"separator",
        		"wrappingBreakpoint",
        		"servoyToolbarItem",
